<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="analitics" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="editor_url" value="https://doc.onlyoffice.com" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="files.docservice.secret.header" value="AuthorizationJwt" xdt:Transform="Insert" xdt:Locator="Match(key)"/>
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors mode="On" xdt:Transform="SetAttributes" />
  </system.web>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="HTTP to HTTPS" enabled="true" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>